{
  "name": "circleci-api",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "circleci",
    "ci",
    "api",
    "typescript"
  ],
  "main": "dist/circleci-api.cjs.js",
  "module": "dist/circleci-api.es5.js",
  "browser": "dist/circleci-api.umd.min.js",
  "typings": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Jordon de Hoog <jordon.dehoog@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordond/circleci-api"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint:lib": "tslint --project ./tsconfig.json -t codeFrame 'src/**/*.ts'",
    "lint:tests": "tslint --project ./tsconfig.test.json -t codeFrame 'test/**/*.ts'",
    "lint": "npm run lint:lib && npm run lint:tests",
    "clean": "rimraf dist",
    "predocs": "rimraf docs",
    "docs": "typedoc --out docs --exclude '**/__mocks__/**' --excludeExternals --target es6 --includeDeclarations --excludePrivate --mode modules src",
    "docs:watch": "watch 'yarn docs' ./src",
    "tsc": "tsc --module commonjs",
    "rollup": "rollup -c rollup.config.ts",
    "prebuild": "npm run clean",
    "build": "npm run tsc && npm run rollup",
    "postbuild": "npm run docs",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:prod": "npm run lint && npm run test -- --coverage --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jest": "^23.1.6",
    "@types/node": "^10.0.3",
    "colors": "^1.1.2",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.0",
    "cross-env": "^5.0.1",
    "cz-conventional-changelog": "^2.0.0",
    "greenkeeper-lockfile": "^1.15.1",
    "husky": "^1.0.0",
    "jest": "^23.4.1",
    "lint-staged": "^7.1.3",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.13.4",
    "prompt": "^1.0.0",
    "replace-in-file": "^3.0.0-beta.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.66.0",
    "rollup-plugin-commonjs": "^9.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-uglify": "^6.0.0",
    "semantic-release": "^15.9.16",
    "source-map-support": "^0.5.6",
    "travis-deploy-once": "^5.0.1",
    "ts-jest": "23.10.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^8.0.0",
    "typedoc": "^0.12.0",
    "typescript": "^2.9.2",
    "watch": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.test.json",
        "diagnostics": true
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage/",
    "coverageReporters": [
      "lcov",
      "html",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "preset": "ts-jest"
  }
}
